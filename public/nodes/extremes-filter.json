{
  "type": "extremes-filter",
  "label": "Extremes Filter",
  "category": "analysis",
  "icon": "filtericon",
  "color": "red",
  "description": "Filters compounds based on physicochemical thresholds.",
  "defaultInputs": [
    {
      "id": "input-mol-data",
      "name": "Input Compounds",
      "type": "array",
      "required": true,
      "description": "Array containing compound data from CSV"
    }
  ],
  "defaultOutputs": [
    {
      "id": "output-filtered",
      "name": "Filtered Results",
      "type": "array",
      "description": "Output data containing filtered compound results"
    }
  ],
  "configSchema": [
    {
      "key": "mw_threshold",
      "type": "number",
      "label": "MW Maximum Threshold",
      "defaultValue": 400
    },
    {
      "key": "logp_threshold",
      "type": "number",
      "label": "logP Maximum Threshold",
      "defaultValue": 4.0
    }
  ],
  "defaultConfig": {
    "mw_threshold": 300,
    "logp_threshold": 3.0,
    "mapping": {
      "name": "name",
      "MW_Da": "MW_Da",
      "logP": "logP"
    }
  }
}