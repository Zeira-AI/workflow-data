{
    "type": "toxicity-filter",
    "label": "Toxicity Filter",
    "category": "analysis",
    "icon": "filtericon",
    "color": "red",
    "description": "Filter compounds based on physicochemical properties and custom rules.",
    "pricing": {
        "credits": 800,
        "executionTime": 1
    },
    "outputMockLabel": "40 compounds",
    "defaultInputs": [
        {
            "id": "input-mol-data",
            "name": "Input Compounds",
            "type": "array",
            "required": true,
            "tooltip": "Table of compounds."
        }
    ],
    "defaultOutputs": [
        {
            "id": "Compound",
            "name": "Compound",
            "type": "string",
            "tooltip": "Name of the compound."
        },
        {
            "id": "HIA_pct",
            "name": "HIA_pct",
            "type": "number",
            "tooltip": "Human Intestinal Absorption (%)."
        },
        {
            "id": "Caco2_pct",
            "name": "Caco2_pct",
            "type": "number",
            "tooltip": "Caco-2 permeability (%)."
        },
        {
            "id": "Sol_mg_mL",
            "name": "Sol_mg_mL",
            "type": "number",
            "tooltip": "Aqueous solubility (mg/mL)."
        },
        {
            "id": "logP",
            "name": "logP",
            "type": "number",
            "tooltip": "Partition coefficient (octanol/water)."
        },
        {
            "id": "BBB",
            "name": "BBB",
            "type": "number",
            "tooltip": "Blood-brain barrier penetration (0=No, 1=Yes)."
        },
        {
            "id": "PPB_pct",
            "name": "PPB_pct",
            "type": "number",
            "tooltip": "Plasma Protein Binding (%)."
        },
        {
            "id": "Clearance_mL_min_kg",
            "name": "Clearance_mL_min_kg",
            "type": "number",
            "tooltip": "Clearance (mL/min/kg)."
        },
        {
            "id": "CYP_inhib_n",
            "name": "CYP_inhib_n",
            "type": "number",
            "tooltip": "Number of CYP450 isoforms inhibited."
        },
        {
            "id": "RM_risk",
            "name": "RM_risk",
            "type": "number",
            "tooltip": "Reactive metabolite risk (0=No, 1=Yes)."
        },
        {
            "id": "hERG_IC50_uM",
            "name": "hERG_IC50_uM",
            "type": "number",
            "tooltip": "hERG channel inhibition IC₅₀ (µM)."
        },
        {
            "id": "Geno",
            "name": "Geno",
            "type": "number",
            "tooltip": "Genotoxicity (0=Negative, 1=Positive)."
        },
        {
            "id": "LD50_mg_kg",
            "name": "LD50_mg_kg",
            "type": "number",
            "tooltip": "Lethal dose 50% (mg/kg)."
        },
        {
            "id": "NOAEL_mg_kg_d",
            "name": "NOAEL_mg_kg_d",
            "type": "number",
            "tooltip": "No Observed Adverse Effect Level (mg/kg/day)."
        },
        {
            "id": "DevRepro",
            "name": "DevRepro",
            "type": "number",
            "tooltip": "Developmental/Reproductive toxicity (0=No, 1=Yes)."
        },
        {
            "id": "Endocrine",
            "name": "Endocrine",
            "type": "number",
            "tooltip": "Endocrine disruption risk (0=No, 0.5=Possible, 1=Yes)."
        },
        {
            "id": "OralIrrit",
            "name": "OralIrrit",
            "type": "number",
            "tooltip": "Oral irritation score (0-10)."
        },
        {
            "id": "Volatile",
            "name": "Volatile",
            "type": "number",
            "tooltip": "Volatility (0=No, 1=Yes)."
        },
        {
            "id": "Decision",
            "name": "Decision",
            "type": "string",
            "tooltip": "Toxicity filter decision (PASS/PASS*/FAIL)."
        }
    ],
    "configSchema": [
        {
            "key": "ingredientListText",
            "type": [
                "text",
                "file"
            ],
            "label": "Compound List",
            "tooltip": "Compounds for analysis",
            "suggestedField": [
                "smiles",
                "name",
                "Compound"
            ],
            "accept": ".csv,.txt,.smi"
        },
        {
            "key": "model",
            "type": "select",
            "label": "Model",
            "options": [
                {
                    "value": "swiss_adme",
                    "label": "SwissADME"
                },
                {
                    "value": "admet_ai",
                    "label": "ADMET-AI"
                },
                {
                    "value": "preadmet",
                    "label": "preADMET"
                }
            ],
            "tooltip": "Choose which ADMET prediction model to use."
        },
        {
            "key": "hia_range",
            "type": "range",
            "label": "HIA Range (%)",
            "defaultValue": [
                0,
                100
            ],
            "tooltip": "Human Intestinal Absorption range (min - max %)."
        },
        {
            "key": "logp_range",
            "type": "range",
            "label": "logP Range",
            "defaultValue": [
                -3.0,
                5.0
            ],
            "tooltip": "Partition coefficient range (min - max)."
        },
        {
            "key": "ppb_range",
            "type": "range",
            "label": "PPB Range (%)",
            "defaultValue": [
                0,
                100
            ],
            "tooltip": "Plasma Protein Binding range (min - max %)."
        },
        {
            "key": "herg_range",
            "type": "range",
            "label": "hERG IC₅₀ Range (µM)",
            "defaultValue": [
                10,
                500
            ],
            "tooltip": "hERG channel IC₅₀ range (min - max µM)."
        },
        {
            "key": "ld50_range",
            "type": "range",
            "label": "LD₅₀ Range (mg/kg)",
            "defaultValue": [
                100,
                10000
            ],
            "tooltip": "LD₅₀ threshold range (mg/kg)."
        },
        {
            "key": "noael_range",
            "type": "range",
            "label": "NOAEL Range (mg/kg/day)",
            "defaultValue": [
                50,
                5000
            ],
            "tooltip": "NOAEL threshold range (mg/kg/day)."
        },
        {
            "key": "oral_irrit_range",
            "type": "range",
            "label": "Oral Irritation Range",
            "defaultValue": [
                0,
                5
            ],
            "tooltip": "Oral irritation score range (0-10)."
        }
    ],
    "defaultConfig": {
        "hia_range": [
            0,
            100
        ],
        "logp_range": [
            -3.0,
            5.0
        ],
        "ppb_range": [
            0,
            100
        ],
        "herg_range": [
            10,
            500
        ],
        "ld50_range": [
            100,
            10000
        ],
        "noael_range": [
            50,
            5000
        ],
        "oral_irrit_range": [
            0,
            5
        ]
    }
}